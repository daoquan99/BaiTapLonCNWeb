@model ShopCar.Model.PhieuNhap

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Chi tiết</h2>

<div>
    <h4>Phiếu nhập</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mã phiếu nhập
        </dt>
        <dd>
            @Html.DisplayFor(model => model.MaPN)
        </dd>
        <dt>
            Ngày nhập
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NgayNhap)
        </dd>

        <dt>
            Nhà cung cấp
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NhaCC.TenNCC)
        </dd>
        <dt>
            Mã nhà cung cấp
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NhaCC.MaNCC)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Sửa", "Edit", new { id = Model.MaPN }) |
    @Html.ActionLink("Quay lại danh sách", "Index")
</p>
